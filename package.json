    "ci": "npm run lint && npm run build:client && npm run test",
    "workflow": "tsx tools/scripts/test-workflow.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.3",
    "@supabase/supabase-js": "^2.91.0",
    "clsx": "^2.0.0",
