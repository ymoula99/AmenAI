    "jsdom": "^27.4.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "sharp": "^0.32.4",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.21.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.1.0"
