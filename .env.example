# Mode de développement
# Si false, désactive le mode mock
VITE_MOCK_API=false

# Activation de l'API OpenAI réelle
# Si true, appelle OpenAI pour la génération d'images
VITE_USE_OPENAI=true

# Clé API OpenAI
VITE_OPENAI_API_KEY=YOUR_OPENAI_API_KEY

# URL de l'API backend (optionnel)
VITE_API_BASE_URL=/api

# Supabase Configuration
# Pour stocker le catalogue produits dans le cloud
VITE_SUPABASE_URL=YOUR_SUPABASE_URL
VITE_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY
